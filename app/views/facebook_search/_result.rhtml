<div class="result">
	<div class="top">
		<div class="title"><%= result.title %></div>
		<div class="type"><%= result.type %></div>
	</div>
	<div class="description">
		<% if !result.data.nil? %>
			<%= result.data %>
		<% else %>
			<!-- TODO: summary for every datatype without data -->
			<%= result.description %>
		<% end %>
	</div>
</div>
