<% if city.class == Category %>
	<li><%= link_to city.name, country_state_city_path(@country.url_part, @state.url_part, city.url_part) %></li>
<% elsif city.class == City %>
	<li><%= link_to city.name, country_state_city_path(@country.url_part, @state.url_part, city.category.url_part) %></li>
<% end %>
